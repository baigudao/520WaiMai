<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jackie.waimai">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".application.JackieApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_laucher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Holo.Light">

        <activity
            android:name=".activity.Welcome"
            android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.ErWeiMaActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.LoginAndRegister" />
        <activity android:name=".activity.SameActivity" />
        <activity android:name=".activity.FuJinAndJingPingActivity" />
        <activity android:name=".activity.TellActivity" />
        <activity android:name=".activity.LoginTextActivity" />
        <activity android:name=".activity.RestaurantActivity" />
        <activity android:name=".activity.AddAddressActivity"/>
        <activity android:name=".activity.AddressActivity"/>
        <activity android:name=".activity.OpinionFeedbackActivity"/>
        <activity android:name=".activity.SettingActivity"/>

    </application>

</manifest>
